{
  "compilerOptions": {
    "target": "ES5",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "noUnusedLocals": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "jsx": "react-jsx",
    "allowJs": true,
    "sourceMap": true
  },
  "include": [
    "src/**/*",
    "resx.d.ts"
  ]
}